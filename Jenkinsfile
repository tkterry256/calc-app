pipeline{
    agent any
    stages{
        stage('Install Dependencies'){
            // npm install
            steps{
                echo '#####Installing Dependencies#########'
            }
            
        }

        stage('Run Test'){
            // npm run test
            steps{
                echo '#####Running Tests#########'
            }
            
        }

        stage('Run Build'){
            steps{
                echo '#####Running Build#########'
            }
            
        }
    }
}